{
    "name": "LT",
    "dataColors":["#753f97", "#fdb911", "#F61057", "#f3e0ec", "#d9bbf9", "#a9Cef4", "#ec7505", "#8ea604", "#08415c"],
    "background":"#FFFFFF",
    "foreground": "#000000",
    "tableAccent": "#753f97",
	"visualStyles":{
        "*":{
            "*":{
                "*":[{
                    "responsive":true,
				    "wordWrap":true,
                    "fontSize":10,
                    "fontFamily":"Segoe UI Light",
                    "alignment": "Center"
                }],
                "title": [{
                    "show": true,
                    "fontColor": { "solid": { "color": "#753f97"} },
                    "background": "None",
                    "backgroundTransparency": 100,
                    "alignment": "Center",
                    "fontSize": 14,
                    "fontFamily": "Segoe UI Light"
                }]
		    }
	    },
        "lineChart":{
            "*":{
                "legend":[{
                    "legendMarkerRendering":"lineAndMarker"
                }] 
            }
        },
        "donutChart": {
            "*": {
                "legend": [{
                    "show": false,
                    "position": "Top",
                    "showTitle": true,
                    "titleText": "",
                    "legendColor": { "solid": { "color": "#777777"}},
                    "fontFamily": "Segoe UI",
                    "fontSize": 12
                }],
                "labels": [{
                    "show": true,
                    "labelStyle": "Category, data value, percent of total",
                    "color": { "solid": { "color": "#777777"}},
                    "labelDisplayUnits": "0",
                    "labelPrecision": 0,
                    "fontSize": 10,
                    "fontFamily": "DIN",
                    "position": "outside"
                }],
                "slices": [{
                    "innerRadiusRatio": 60
                }]
            }
        },
        "pieChart": {
            "*": {
                "legend": [{
                    "show": false,
                    "position": "Top",
                    "showTitle": true,
                    "titleText": "",
                    "legendColor": { "solid": { "color": "#777777"}},
                    "fontFamily": "Segoe UI",
                    "fontSize": 10
                }],
                "labels": [{
                    "show": true,
                    "labelStyle": "Category, data value, percent of total",
                    "color": { "solid": { "color": "#777777"}},
                    "labelDisplayUnits": "0",
                    "labelPrecision": 0,
                    "fontSize": 10,
                    "fontFamily": "DIN",
                    "position": "outside"
                }]
            }
        },
        "pivotTable": {
            "*": {
                "grid": [{
                    "gridVertical": false,
                    "gridHorizontal": false,
                    "rowPadding": 1,
                    "outlineColor": { "solid": { "color": "#753f97"}},
                    "outlineWeight": 1,
                    "textSize": 10
                }],
                "columnHeaders": [{
                    "fontColor": { "solid": { "color": "#000000"}},
                    "backColor": { "solid": { "color": "#FFFFFF"}},
                    "outline": "BottomOnly",
                    "autoSizeColumnWidth": true,
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10,
                    "alignment": "Center",
                    "urlIcon": true,
                    "wordWrap": true
                }],
                "rowHeaders": [{
                    "fontColor": { "solid": { "color": "#000000"}},
                    "backColor": { "solid": { "color": "#FFFFFF"}},
                    "outline": "RightOnly",
                    "stepped": true,
                    "steppedLayoutIndentation": 12,                 
                    "urlIcon": true,
                    "wordWrap": true,
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10,
                    "alignment": "Left"
                }],
                "values": [{
                    "fontColorPrimary": { "solid": { "color": "#000000"}},
                    "backColorPrimary": { "solid": { "color": "#FFFFFF"}},
                    "fontColorSecondary": { "solid": { "color": "#000000"}},
                    "backColorSecondary": { "solid": { "color": "#f3e0ec"}},
                    "bandedRowHeaders": true,
                    "valuesOnRow": false,
                    "outline": "None",
                    "urlIcon": false,
                    "alignment": "Center",
                    "wordWrap": true,
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10
                }],
                "subTotals": [{
                    "rowSubtotals": true,
                    "columnSubtotals": true,
                    "fontColor": { "solid": { "color": "000000"}},
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10,
                    "applyToHeaders": true,
                    "rowSubtotalsPosition": "Bottom",
                    "perRowLevel": true,
                    "perColumnLevel": true                  
                }],
                "columnFormatting": [{
                    "fontColor": { "solid": { "color": "000000"}},
                    "fontFamily": "Segoe UI Light",
                    "alignment": "Center",
                    "styleHeader": true,
                    "styleValues": true,
                    "styleTotal": true,
                    "styleSubtotals": true
                }],
                "total": [{
                    "fontColor": { "solid": { "color": "#000000"}},
                    "fontFamily": "Segoe UI Light",
                    "backColor": { "solid": { "color": "#FFFFFF"}},
                    "applyToHeaders": true,
                    "fontSize": 10
                }]
            }
        },
        "tableEx": {
            "*": {
                "grid": [{
                    "gridVertical": false,
                    "gridHorizontal": false,
                    "rowPadding": 1,
                    "outlineColor": { "solid": { "color": "#753f97"}},
                    "outlineWeight": 1,
                    "textSize": 10
                }],
                "columnHeaders": [{
                    "fontColor": { "solid": { "color": "#000000"}},
                    "backColor": { "solid": { "color": "#FFFFFF"}},
                    "outline": "BottomOnly",
                    "autoSizeColumnWidth": true,
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10,
                    "alignment": "Center",
                    "urlIcon": true,
                    "wordWrap": true
                }],
                "values": [{
                    "fontColorPrimary": { "solid": { "color": "#000000"}},
                    "backColorPrimary": { "solid": { "color": "#FFFFFF"}},
                    "fontColorSecondary": { "solid": { "color": "#000000"}},
                    "backColorSecondary": { "solid": { "color": "#f3e0ec"}},
                    "bandedRowHeaders": true,
                    "valuesOnRow": false,
                    "outline": "None",
                    "urlIcon": false,
                    "wordWrap": true,
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10
                }],
                "subTotals": [{
                    "rowSubtotals": true,
                    "columnSubtotals": true,
                    "fontColor": { "solid": { "color": "000000"}},
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10,
                    "applyToHeaders": true,
                    "rowSubtotalsPosition": "Bottom",
                    "perRowLevel": true,
                    "perColumnLevel": true                  
                }],
                "columnFormatting": [{
                    "fontColor": { "solid": { "color": "000000"}},
                    "fontFamily": "Segoe UI Light",
                    "alignment": "Center",
                    "styleHeader": true,
                    "styleValues": true,
                    "styleTotal": true,
                    "styleSubtotals": true
                }],
                "total": [{
                    "totals": true,
                    "fontColor": { "solid": { "color": "#000000"}},
                    "backColor": { "solid": { "color": "#FFFFFF"}},
                    "outline": "None",
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10
                }]
            }
        },
        "clusteredColumnChart": {
            "*": {
                "general": [{
                    "responsive": true
                }],
                "legend": [{
                    "show": false,
                    "position": "Top",
                    "showTitle": true,
                    "titleText": "",
                    "labelColor": { "solid": { "color": "#777777"}},
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10
                }],
                "categoryAxis": [{
                    "show": true,
                    "labelColor": { "solid": { "color": "#777777"}},
                    "fontSize": 10,
                    "fontFamily": "Segoe UI Light",
                    "preferredCategoryWidth": 30,
                    "maxMarginFactor": 28,
                    "innerPadding": 22,
                    "concatenateLabels": true,
                    "showAxisTitle": false,
                    "axisStyle": "showTitleOnly",
                    "titleText": "",
                    "titleColor": { "solid": { "color": "#777777"}},
                    "titleFontSize": 10,
                    "titleFontFamily": "Segoe UI"
                }],
                "valueAxis": [{
                    "show": true,
                    "position": "Right",
                    "axisScale": "Linear",
                    "start": "Auto",
                    "end": "Auto",
                    "labelColor": { "solid": { "color": "#777777"}},
                    "fontSize": 10,
                    "fontFamily": "Segoe UI Light",
                    "labelDisplayUnits": "Auto",
                    "labelPrecision": "Auto",
                    "showAxisTitle": false,
                    "axisStyle": "showTitleOnly",
                    "color": { "solid": { "color": "#777777"}},
                    "titleFontSize": 10,
                    "titleFontFamily": "Segoe UI Light",
                    "gridlineShow": true,
                    "gridlineColor": { "solid": { "color": "#777777"}},
                    "gridlineThickness": 1,
                    "gridlineStyle": "solid"
                }],
                "dataPoint": [{
                    "defaultColor": { "solid": { "color": "#448822"}}
                }],
                "labels": [{
                    "show": true,
                    "color": { "solid": { "color": "#777777"}},
                    "labelDisplayUnits": "0",
                    "labelPrecision": 1,
                    "labelOrientation": "horizontal",
                    "labelPosition": "OutsideEnd",
                    "fontSize": 10,
                    "fontFamily": "Segoe UI Light",
                    "enableBackground": false,
                    "backgroundColor": { "solid": { "color": "000000"}},
                    "backgroundTransparency": 100
                }],     
                "plotArea": [{
                    "transparency": 100
                }]
            }
        },
        "clusteredBarChart": {
            "*": {
                "general": [{
                    "responsive": true
                }],
                "legend": [{
                    "show": false,
                    "position": "Top",
                    "showTitle": true,
                    "titleText": "",
                    "labelColor": { "solid": { "color": "#777777"}},
                    "fontFamily": "Segoe UI Light",
                    "fontSize": 10
                }],
                "categoryAxis": [{
                    "show": true,
                    "position": "Left",
                    "labelColor": { "solid": { "color": "#777777"}},
                    "fontSize": 10,
                    "fontFamily": "Segoe UI Light",
                    "preferredCategoryWidth": 30,
                    "maxMarginFactor": 28,
                    "innerPadding": 22,
                    "concatenateLabels": true,
                    "showAxisTitle": false,
                    "titleColor": { "solid": { "color": "#777777"}},
                    "titleText": "",
                    "titleFontSize": 10,
                    "titleFontFamily": "Segoe UI"
                }],
                "valueAxis": [{
                    "show": true,
                    "position": "Right",
                    "axisScale": "Linear",
                    "start": "Auto",
                    "end": "Auto",
                    "labelColor": { "solid": { "color": "#777777"}},
                    "fontSize": 10,
                    "fontFamily": "Segoe UI Light",
                    "labelDisplayUnits": "0",
                    "labelPrecision": "Auto",
                    "showAxisTitle": false,
                    "axisStyle": "showTitleOnly",
                    "color": { "solid": { "color": "#777777"}},
                    "titleText": "",
                    "titleFontSize": 10,
                    "titleFontFamily": "Segoe UI Light",
                    "gridlineShow": true,
                    "gridlineColor": { "solid": { "color": "#777777"}},
                    "gridlineThickness": 1,
                    "gridlineStyle": "dashed"
                }],
                "dataPoint": [{
                    "defaultColor": { "solid": { "color": "#448822"}}
                }],
                "labels": [{
                    "show": true,
                    "color": { "solid": { "color": "#777777"}},
                    "labelDisplayUnits": "0",
                    "labelPrecision": 1,
                    "labelPosition": "OutsideEnd",
                    "fontSize": 10,
                    "fontFamily": "Segoe UI Light",
                    "enableBackground": false,
                    "backgroundTransparency": 100
                }],     
                "plotArea": [{
                    "transparency": 100
                }]
            }
        },
        "card": {
            "*": {
                "labels": [{
                    "color": { "solid": { "color": "#000000"}},
                    "labelDisplayUnits": "0",
                    "labelPrecision": 0,
                    "fontSize": 14,
                    "fontFamily": "Segoe UI Light"
                }],
                "categoryLabels": [{
                    "show": false,
                    "color": { "solid": { "color": "#01B8AA"}},
                    "fontSize": 14,
                    "fontFamily": "Segoe UI Light"
                }],
                "wordWrap": [{
                    "show": true
                }],
                "background": [{
                        "show": false,
                        "color": {
                            "solid": {
                                "color": "#000000"
                            }
                        },
                        "transparency": 100
                    }
                ],
                  "title": [{
                    "show": true,
                    "fontColor": { "solid": { "color": "#753f97" } },
                    "background": { "solid": { "color": "#062b60" } },
                    "backgroundTransparency": 100,
                    "alignment": "Center",
                    "fontSize": 14,
                    "fontFamily": "Segoe UI"
                  }],
                  "border": [{
                    "show": false,
                    "color": { "solid": { "color": "#062b60"}}
                  }]
            }
        }
    }
}